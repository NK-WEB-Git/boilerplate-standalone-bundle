{
    "name": "nkweb/custom-bundle",
    "description": "A boilerplate of a standalone bundle symfony with structure files and configuration settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Boris Stephane NKUINJEU",
            "email": "nkuinjeustephane@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Custom\\StandaloneBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Custom\\StandaloneBundle\\Tests\\": "./Tests/"
        }
    },
    "require": {
        "symfony/http-kernel": "^3.3"
    },
    "require-dev": {
        "symfony/framework-bundle": "^3.3",
        "symfony/yaml": "^3.3",
        "symfony/console": "^3.3",
        "symfony/process": "^3.3",
        "symfony/web-server-bundle": "^3.3",
        "phpunit/phpunit": "^5.7",
        "symfony/browser-kit": "^3.3"
    }
}
